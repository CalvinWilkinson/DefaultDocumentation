<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<Description>Api to extend the creation of documentation of DefaultDocumentation.</Description>
		<RootNamespace>DefaultDocumentation</RootNamespace>
		<TargetFramework>netstandard2.0</TargetFramework>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="ICSharpCode.Decompiler" Version="7.2.0.6844" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="NLog" Version="4.7.14" />
	</ItemGroup>
</Project>
